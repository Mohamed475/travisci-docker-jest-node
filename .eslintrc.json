{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "no-console": "off",
        "no-unused-vars": "off"
    }
}
